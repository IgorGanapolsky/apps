{
  "expo": {
    "name": "Apps Template",
    "slug": "apps-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.igorganapolsky.appstemplate",
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "package": "com.igorganapolsky.appstemplate",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "apps-template",
    "experiments": {
      "typedRoutes": true
    }
  }
}
